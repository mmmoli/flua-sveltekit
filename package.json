{
	"name": "flua-sveltekit",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"db:generate": "drizzle-kit generate:pg --config drizzle.config.ts",
		"db:migrate": "drizzle-kit push:pg --config drizzle.config.ts",
		"db:studio": "drizzle-kit studio --config drizzle.config.ts"
	},
	"devDependencies": {
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-vercel": "^4.0.2",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@tanstack/svelte-query-devtools": "^5.14.0",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"autoprefixer": "^10.4.16",
		"drizzle-kit": "^0.20.6",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.30.0",
		"pg": "^8.11.3",
		"postcss": "^8.4.32",
		"postcss-load-config": "^5.0.2",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"prettier-plugin-tailwindcss": "^0.5.9",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tailwindcss": "^3.3.6",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.3",
		"vitest": "^0.32.2"
	},
	"type": "module",
	"dependencies": {
		"@neondatabase/serverless": "^0.6.0",
		"@tanstack/react-query-devtools": "^5.14.0",
		"@tanstack/svelte-query": "^5.14.0",
		"@trpc/client": "^10.44.1",
		"@trpc/server": "^10.44.1",
		"clerk-sveltekit": "^0.3.0",
		"dotenv": "^16.3.1",
		"drizzle-orm": "^0.29.1",
		"rich-domain": "^1.19.1",
		"slugify": "^1.6.6",
		"trpc-svelte-query-adapter": "^2.2.1",
		"trpc-sveltekit": "^3.5.22",
		"zod": "^3.22.4",
		"zod-validation-error": "^2.1.0"
	}
}
